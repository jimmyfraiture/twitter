<div class="col-md-4">
	<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  	<div class="form-group">
    <label>Username</label>
    <%= f.text_field :username, autofocus: true, class: "form-control", placeholder: "Entre your username" %>
    </div>
    <div class="form-group">
    <label>Email</label>
    <%= f.email_field :email, class: "form-control", placeholder: "Entrez your email" %>
    </div>
    <div class="form-group">
    <label>Password</label>
    <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Entre your password" %>
    </div>
    <div class="form-group">
    <label>Password confirmation</label>
    <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Entre your password" %>
    </div>
    <%= f.submit "Create", class: "btn btn-default" %>
	<% end %>
</div>
