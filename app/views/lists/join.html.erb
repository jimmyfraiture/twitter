<div class="page-header">
  <div class="col-md-5"> </div>
    <h1>Twitter <small>Join a list</small></h1>
</div>


</br>
<div class="col-md-6">
<%= form_for @ld do |f| %>
    <div class="form-group">
      <label>ID list</label>
      <%= f.text_field :list_id, class: "form-control", placeholder: "Entrez l'ID de la list" %>
    </div>
    <div class="form-group">
      <label>Your ID</label>
      <%= f.text_field :user_id, class: "form-control", placeholder: "Entrez votre ID" %>
    </div>
    <%= link_to 'Join', lists_jupdate_path() , class: "btn btn-success" %>
<% end %>
</div>