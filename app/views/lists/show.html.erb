<div class="col-md-1">
</div>
<div class="col-md-4">
    <div class="form-group">
    <label>Name : </label>
    <%= @list.name %>
    </div>
    <div class="form-group">
    <label>Description : </label>
    <%= @list.comment %>
    </div>



    <div class="col-md-12">
        <div class="col-md-6 col-md-offset-3">
            <table class="table table-striped">
                <tr><th>id</th><th>Name</th><th></th></tr>
                <% @users.each do |user| %>
                <tr><td><%= user.id %></td><td><%= user.username %></td><td><%= link_to 'Show User', user_path(user) , class: "btn btn-info" %></td></tr>
                <% end %>
            </table>
        </div>
    </div>

    <%= link_to 'Join', new_list_distribution_path() , class: "btn btn-success" %>
</div>
